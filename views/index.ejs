<body>

    <div class="container-fluid">
        <div class="row">
            <div class="col-sm-4"></div>

            <div class="col-sm-4 chatwindow">

                <br><br><br>

                <!-- text popup from socketio stuff -->
                <ul id="messages"></ul>

                <br><br><br>

                <form role="form" onsubmit="">
                    <div class="form-group float-label-control label-bottom">
                        <label for="">Start Typing...</label>
                        <input type="text" class="form-control" placeholder="Start Typing...">
                    </div>
                </form>

            </div>

        </div>
    </div>




    <!-- Script Imports (After page load) -->
    <!-- Local -->
    <script src="jquery/1.11.3/jquery.min.js"></script>
    <script src="bootstrap-3.3.6-dist/js/bootstrap.min.js"></script>
    <script src="javascripts/floatlabelpatternform.js"></script>

    <!-- SockJS -->
    <script src="//cdn.jsdelivr.net/sockjs/1.0.3/sockjs.min.js"></script>

    <script>
        var sock = new SockJS('http://localhost:3000');

        sock.onopen = function() {
            console.log('open');
        };
        sock.onmessage = function(e) {
            console.log('message', e.data);
        };
        sock.onclose = function() {
            console.log('close');
        };

        sock.send('test');
        sock.close();
    </script>




